modules = ["nodejs-20", "web"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Launcher"

[[workflows.workflow]]
name = "Launcher"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev:launcher"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 32961
externalPort = 3000

[[ports]]
localPort = 35075
externalPort = 3002

[[ports]]
localPort = 42411
externalPort = 3001

[[ports]]
localPort = 45511
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "run", "preview"]
build = ["npm", "run", "build:launcher"]
